{
  "name": "react-svg-icon",
  "version": "1.0.0",
  "description": "A React component that renders SVG to the site's favicon",
  "main": "build/cjs/index.js",
  "module": "build/esm/index.js",
  "types": "build/types/index.js",
  "scripts": {
    "build": "npm run clean && npm run build:esm && npm run build:cjs && npm run build:types",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:types": "tsc -p tsconfig.types.json",
    "clean": "rimraf build"
  },
  "keywords": [
    "react",
    "favicon",
    "icon",
    "component"
  ],
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "author": "Benjamin Swart",
  "homepage": "https://github.com/SvizelPritula/react-svg-icon/blob/main/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/SvizelPritula/react-svg-icon.git"
  },
  "license": "MIT",
  "devDependencies": {
    "rimraf": "^3.0.2",
    "typescript": "^4.3.4"
  }
}